{
  "type": "extension",
  "name": "http_server_python",
  "version": "0.11.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.10"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "**.py",
      "requirements.txt",
      "README.md"
    ]
  },
  "api": {
    "property": {
      "properties": {
        "listen_addr": {
          "type": "string"
        },
        "listen_port": {
          "type": "int32"
        }
      }
    }
  }
}